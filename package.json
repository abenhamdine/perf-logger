    "it-each": "0.4.0",
    "jsdoc": "3.6.3",
    "mocha": "5.2.0"
  },
  "dependencies": {
    "chai": "~4.2.0",
    "colors": "~1",
    "lodash": "~4",
    "moment": "2.24.0",
